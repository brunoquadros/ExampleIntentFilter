<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.ceduphh.bruno.exampleintentfilter" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/drag" >
        <activity
            android:name=".Menu"
            android:label="Menu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Tela1" android:label="Tela1" >
            <intent-filter>
                <action android:name="ACAO_TESTE" />
                <category android:name="android.intent.category.DEFAUlT" />
            </intent-filter>
        </activity>
        <activity  android:name="Tela2" android:label="Tela2" >
            <intent-filter>
                <action android:name="ACAO_TESTE" />
                <category android:name="android.intent.category.DEFAUlT" />
                <category android:name="CATEGORIA_TESTE" />
            </intent-filter>
        </activity>
        <activity android:name=".Tela3" android:label="Tela3" >
            <intent-filter>
                <action android:name="ABRIR_TELA" />
                <category android:name="android.intent.category.DEFAUlT" />
                <category android:name="CATEGORIA_3" />
            </intent-filter>
        </activity>
        <activity android:name="Tela4" android:label="Tela4" >
            <intent-filter>
                <action android:name="ABRIR_TELA" />
                <category android:name="android.intent.category.DEFAUlT" />
                <category android:name="CATEGORIA_DUPLICADA" />
            </intent-filter>
        </activity>
        <activity android:name=".Tela5" android:label="Tela5" >
            <intent-filter>
                <action android:name="ABRIR_TELA" />
                <category android:name="android.intent.category.DEFAUlT" />
                <category android:name="CATEGORIA_DUPLICADA" />
            </intent-filter>
        </activity>
    </application>

</manifest>
